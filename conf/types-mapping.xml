		</map>
		<map comment="SensorMultilevel (air temperature)">
			<z-wave command-class="49" index="1" />
			<beeeon type="temperature" />
		</map>
		<map comment="SensorMultilevel (luminance)">
			<z-wave command-class="49" index="3" />
			<beeeon type="luminance" />
		</map>
		<map comment="SensorMultilevel (power)">
			<z-wave command-class="49" index="4" />
