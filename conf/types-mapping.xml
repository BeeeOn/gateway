		</map>
		<map comment="SensorMultilevel (soil humidity)">
			<z-wave command-class="49" index="41" />
			<beeeon type="humidity" />
		</map>
		<map comment="SensorMultilevel (outer temperature)">
			<z-wave command-class="49" index="34" />
			<beeeon type="temperature,outer" />
		</map>
		<map comment="SensorMultilevel (particular matter 2.5)">
			<z-wave command-class="49" index="35" />
			<beeeon type="pm25" />
		</map>
		<map comment="Battery">
			<z-wave command-class="128" />
			<beeeon type="battery" />
		</map>
	</z-wave-mapping>
</types-mapping>
