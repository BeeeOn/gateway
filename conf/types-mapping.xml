		</map>
		<map comment="SensorMultilevel (soil temperature)">
			<z-wave command-class="49" index="24" />
			<beeeon type="temperature" />
		</map>
		<map comment="SensorMultilevel (ultraviolet)">
			<z-wave command-class="49" index="27" />
			<beeeon type="ultraviolet" />
		</map>
		<map comment="SensorMultilevel (noise)">
			<z-wave command-class="49" index="30" />
